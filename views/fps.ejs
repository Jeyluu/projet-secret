<%- include('partials/header') %>



                
                    <h2 class="titre-page">Les jeux FPS</h2>
            


        <div class="container">
            

            <div class="row">


                <div class="col-md-10">
                    <% carteJeu.forEach(function(carte){%>
                        <div class="article">
                            <div class="image-article">
                                <img src="/images/uploads/<%=carte.image%>" alt="">
                            </div>
                            <div class="texte-article">
                                <div class="titre-article">
                                    <h4>
                                        <%=carte.titre%>
                                    </h4>
                                </div>
                                <div class="description-article">
                                    <p>
                                        <%=carte.contenu%>
                                    </p>
                                </div>
                            </div>

                        </div>
                        <%})%>
                </div>


            </div>
        </div>
    


    <%- include('partials/footer') %>