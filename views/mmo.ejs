



    <%- include('partials/header') %>
        <div class="container">
            <div class="row">

                <div class="col-md-12">
                    <h2>Les jeux Massivement Multijoueur Online</h2>
                </div>

            </div>

            <div class="row">

                <div class="col-md-8">
                    <% carteJeu.forEach(function(carte){%>
                        <div class="article">
                            <div class="image-article">
                                <img src="/images/uploads/<%=carte.image%>" alt="" style="width: 60%;">
                            </div>
                            <div class="texte-article">
                                <div class="titre-article">
                                    <h4>
                                        <%=carte.titre%>
                                    </h4>
                                </div>
                                <div class="description-article">
                                    <p>
                                        <%=carte.contenu%>
                                    </p>
                                </div>
                            </div>

                        </div>
                        <%})%>

                </div>


            </div>
        </div>
        <%- include('partials/footer') %>
